				</dict>
				<dict>
					<key>text</key>
					<string>SENSOR ALARM</string>
					<key>description</key>
					<string>Установка Входа 2 в режим тревожной кнопки</string>
				</dict>
				<dict>
					<key>text</key>
					<string>SENSOR HOOD</string>
