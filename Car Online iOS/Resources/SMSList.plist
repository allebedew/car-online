	<array>
		<dict>
			<key>title</key>
			<string>Основные настройки</string>
			<key>items</key>
			<array>
				<dict>
					<key>text</key>
					<string>[C-Code]</string>
					<key>description</key>
					<string>Инициализация MS-PGSM Спутник</string>
				</dict>
				<dict>
					<key>text</key>
					<string>[C-Code] INIT</string>
